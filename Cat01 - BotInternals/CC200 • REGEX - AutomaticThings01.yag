  {{removeRoleID 1030956478384832664 300}}
  {{if $platine := dbGet 0 "platinium"}} {{deleteMessage 1030950947993702451 (toInt $platine.Value)}} {{end}}
  {{dbSet 0 "platinium" (toString .Message.ID)}}
{{end}}

